#include "charybdis_handwired.dtsi"

&default_transform {
    col-offset = <5>;
};

// https://zmk.dev/docs/development/hardware-integration/new-shield implies this order should be flipped,
// but I wired according to ./charybdis_wiring_col_row.png
&kscan0 {
    col-gpios
        = <&pro_micro 15 GPIO_ACTIVE_HIGH>
        , <&pro_micro 14 GPIO_ACTIVE_HIGH>
        , <&pro_micro 16 GPIO_ACTIVE_HIGH>
        , <&pro_micro 8 GPIO_ACTIVE_HIGH>
        , <&pro_micro 7 GPIO_ACTIVE_HIGH>
        ;
};